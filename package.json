{
  "name": "pixelhue",
  "displayName": "PixelHue",
  "description": "A colorful and vibrant theme for Visual Studio Code inspired by pixel art.",
  "version": "0.0.1",
  "publisher": "DuaaBatool",  
  "engines": {
    "vscode": "1.0.0" 
